#include <iostream>
using namespace std;
int main() 
{
	
	int num1, num2;
	num1 = 12;
	num2 = 15;
	cout <<  "SIMPLE MATHEMATICS"
<< endl << endl;
    cout << "NUM 1 : 27" << endl;
    cout << "NUM 2 : 15" << endl;
    
    cout << "RESULT" << endl;
    cout << "SUM IS EQUAL TO : 42" << endl;
    cout << "DIFFERENCE IS EQUAL TO : 12" << endl;
    cout << "PRODUCT IS EQUAL TO : 405" << endl;
    cout << " QUOTIENT IS EQUAL TO : 1.8" << endl;
    return 0;
} 
